<?xml version="1.0"?>
<sdf version="1.4">
	<world name="default">
		<!-- A global light source -->
		<include>
			<uri>model://sun</uri>
		</include>

		<!-- A ground plane -->
		<include>
			<uri>model://ground_plane</uri>
		</include>

		<!-- World physics -->
      <physics type="ode">
			<real_time_update_rate>1000.0</real_time_update_rate>
			<max_step_size>0.001</max_step_size>
			<real_time_factor>1</real_time_factor>
			<ode>
				<solver>
					<type>quick</type>
					<iters>150</iters>
					<precon_iters>0</precon_iters>
					<sor>1.400000</sor>
					<use_dynamic_moi_rescaling>1</use_dynamic_moi_rescaling>
				</solver>
				<constraints>
					<cfm>0.00001</cfm>
					<erp>0.2</erp>
					<contact_max_correcting_vel>2000.000000</contact_max_correcting_vel>
					<contact_surface_layer>0.01000</contact_surface_layer>
				</constraints>
			</ode>
		</physics>

		<!-- Load model -->
		<model name="turtlebot3_square">
			<pose frame="">0 0 0 0 -0 0</pose>

			<!-- Wall 1-->
      <link name="wall1">
				<collision name="wall1_collision">
					<geometry>
						<box>
							<size>5 0.15 0.5</size>
							<!--<size>10 0.3 1</size>-->
						</box>
					</geometry>
					<pose frame="">0 0 0.25 0 -0 0</pose>
				</collision>
				<visual name="wall1_visual">
					<pose frame="">0 0 0.25 0 -0 0</pose>
					<geometry>
						<box>
							<size>5 0.15 0.5</size>
							<!--<size>10 0.3 1</size>-->
						</box>
					</geometry>
					<material><ambient>1 1 1 1</ambient></material>
				</visual>
				<!--<pose frame="">0 4.85 0 0 -0 0</pose>-->
				<pose frame="">0 2.425 0 0 -0 0</pose>
			</link>
      <!-- Wall 2 -->
      <link name="wall2">
				<collision name="wall2_collision">
					<geometry>
						<box>
							<size>5 0.15 0.5</size>
							<!--<size>10 0.3 1</size>-->
						</box>
					</geometry>
					<pose frame="">0 0 0.25 0 -0 0</pose>
				</collision>
				<visual  name="wall2_visual">
					<pose frame="">0 0 0.25 0 -0 0</pose>
					<geometry>
						<box>
							<size>5 0.15 0.5</size>
							<!--<size>10 0.3 1</size>-->
						</box>
					</geometry>
					<material><ambient>1 1 1 1</ambient></material>
				</visual>
				<!--<pose frame="">-4.85 0 0 0 -0 1.5708</pose>-->
				<pose frame="">-2.425 0 0 0 -0 1.5708</pose>
			</link>
      <!-- Wall 3 -->
      <link name="wall3">
				<collision name="wall3_collision">
					<geometry>
						<box>
							<size>5 0.15 0.5</size>
							<!--<size>10 0.3 1</size>-->
						</box>
					</geometry>
					<pose frame="">0 0 0.25 0 -0 0</pose>
				</collision>
				<visual name="wall3_visual">
					<pose frame="">0 0 0.25 0 -0 0</pose>
					<geometry>
						<box>
							<size>5 0.15 0.5</size>
							<!--<size>10 0.3 1</size>-->
						</box>
					</geometry>
					<material><ambient>1 1 1 1</ambient></material>
				</visual>
				<!--<pose frame="">0 -4.85 0 0 -0 3.14159</pose>-->
				<pose frame="">0 -2.425 0 0 -0 3.14159</pose>
			</link>
      <!-- Wall 4 -->
      <link name="wall4">
				<collision name="wall4_collision">
					<geometry>
						<box>
							<size>5 0.15 0.5</size>
							<!--<size>10 0.3 1</size>-->
						</box>
					</geometry>
					<pose frame="">0 0 0.25 0 -0 0</pose>
				</collision>
				<visual name="wall4_visual">
					<pose frame="">0 0 0.25 0 -0 0</pose>
					<geometry>
						<box>
							<size>5 0.15 0.5</size>
							<!--<size>10 0.3 1</size>-->
						</box>
					</geometry>
					<material><ambient>1 1 1 1</ambient></material>
				</visual>
				<!--<pose frame="">4.85 0 0 0 0 -1.5708</pose>-->
				<pose frame="">2.425 0 0 0 0 -1.5708</pose>
			</link>

			<static>1</static>
		</model>

	<!-- Objects -->
    <population name="box_population1">
      <model name="box">
        <pose>0 0 0.1 0 0 0</pose>
        <static>0</static>
        <link name="link">
          <inertial>
            <mass>0.001</mass>
            <inertia>
              <ixx>0.083</ixx><ixy>0.0</ixy><ixz>0.0</ixz>
              <iyy>0.083</iyy><iyz>0.0</iyz>
              <izz>0.083</izz>
            </inertia>
          </inertial>
		  
         <collision name="collision">
            <geometry>
               <box>
                  <size>0.07 0.07 0.1</size>
               </box>
            </geometry>
            <surface>
               <friction>
                  <ode>
                     <mu>0.1</mu>
                  </ode>
               </friction>
            </surface>
         </collision>
         <visual name="visual">
            <geometry>
               <box>
                  <size>0.07 0.07 0.1</size>
               </box>
            </geometry>
         </visual>
        </link>
      </model>
      <pose>0 0 0 0 0 0</pose>
      <box>
        <size>3.5 3.5 0.01</size>
      </box>
      <model_count>5</model_count>
      <distribution>
        <type>random</type>
      </distribution>
    </population>

    <!-- Scene stuffs -->
		<scene>
			<ambient>0.4 0.4 0.4 1</ambient>
			<background>0.7 0.7 0.7 1</background>
			<shadows>true</shadows>
		</scene>
		<gui fullscreen="0">
			<camera name="user_camera">
				<pose>0.0 0.0 17.0 0 1.5708 0</pose>
				<view_controller>orbit</view_controller>
			</camera>
		</gui>
	</world>
</sdf>
